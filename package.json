{
  "name": "pyoncord",
  "license": "BSD-3-Clause",
  "description": "A Discord React Native client mod",
  "publishConfig": {
    "directory": "lib",
    "typings": "index.d.ts"
  },
  "scripts": {
    "build": "node build.mjs",
    "deploy:root": "node build.mjs --deploy-root",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern src/*",
    "prepublishOnly": "if [ -d lib ]; then rm -r lib; fi && tsc && cp package.json lib && cp LICENSE lib && cp README.md lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyoncord/pyoncord.git"
  },
  "bugs": {
    "url": "https://github.com/pyoncord/pyoncord/issues"
  },
  "keywords": [
    "discord",
    "discordrn",
    "react-native"
  ],
  "author": "amsyarasyiq",
  "devDependencies": {
    "@swc/core": "^1.3.76",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.70.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.56.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-path-alias": "^1.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.3",
    "@typescript-eslint/typescript-estree": "^6.18.1",
    "core^\u001b[200~1.3.76~": "link:@swc/core^\u001b[200~1.3.76~",
    "spitroast": "^1.4.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    }
  },
  "packageManager": "pnpm@7.0.0"
}