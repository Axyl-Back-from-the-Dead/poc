{
  "name": "pyoncord",
  "version": "0.1.1",
  "license": "BSD-3-Clause",
  "description": "A Discord React Native client mod",
  "publishConfig": {
    "directory": "lib",
    "typings": "index.d.ts"
  },
  "scripts": {
    "build": "node build.mjs --release",
    "deploy:root": "node build.mjs --deploy-root",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern src/*",
    "prepublishOnly": "rm -rf lib && tsc && tsc-alias && cp {package.json,README.md,LICENSE} lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyoncord/pyoncord.git"
  },
  "bugs": {
    "url": "https://github.com/pyoncord/pyoncord/issues"
  },
  "keywords": [
    "discord",
    "discordrn",
    "react-native"
  ],
  "author": "amsyarasyiq",
  "devDependencies": {
    "@swc/core": "^1.3.59",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.70.13",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "esbuild": "^0.17.19",
    "esbuild-plugin-globals": "^0.2.0",
    "eslint": "^8.41.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-path-alias": "^1.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.1",
    "spitroast": "^1.4.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    }
  },
  "packageManager": "pnpm@7.0.0"
}
